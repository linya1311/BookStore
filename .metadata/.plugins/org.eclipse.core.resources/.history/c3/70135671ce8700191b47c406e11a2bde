<script>
$('.has-error').change(function () {
    $(this).removeClass('has-error');
});
$('#username').on('input', function () {
    if ($('#reg_type').val() === 'email') {
        var username = $('#username').val();
        if (username.indexOf('@') >= 0) {
            $('#nickname').val(username.split('@')[0]);
        } else {
            $('#nickname').val('');
        }
    }
});

$('.eye').on('click', function () {
    var strShowPasswd = "顯示密碼";
    var strHidePasswd = "隱藏密碼";

    $(this).toggleClass('eye-show eye-hide');
    if ($(this).hasClass('eye-show')) {
        $('#password').attr('type', 'text');
        $(this).attr('title', strHidePasswd);
    } else {
        $('#password').attr('type', 'password');
        $(this).attr('title', strShowPasswd);
    }
})

$(() => {
    grecaptcha.ready(() => {
        grecaptcha.execute('6LfpFXgUAAAAAMBOI9nrk9YEbh61kiEvx8qwdSmn', {action: 'login'}).then((token) => {
            $('#g-recaptcha-response').val(token);
        });
    });
});
</script>
